{
  "mcpServers": {
    "example-mcp-server": {
      "command": "node",
      "args": [
        "/path/to/MCP_EXAMPLE_PROJECT/dist/index.js",
        "--stdio"
      ],
      "env": {
        "NODE_ENV": "production",
        "LOG_LEVEL": "info"
      }
    },
    "example-mcp-server-dev": {
      "command": "tsx",
      "args": [
        "/path/to/MCP_EXAMPLE_PROJECT/src/index.ts"
      ],
      "env": {
        "NODE_ENV": "development",
        "LOG_LEVEL": "debug"
      }
    }
  }
}
