{
  "mcpServers": {
    "example-mcp-server": {
      "command": "/Users/username/.nvm/versions/node/v22.16.0/bin/node",
      "args": [
        "/full/path/to/MCP_EXAMPLE_PROJECT/dist/index.js",
        "--stdio"
      ],
      "cwd": "/full/path/to/MCP_EXAMPLE_PROJECT",
      "env": {
        "NODE_ENV": "production",
        "LOG_LEVEL": "info"
      }
    },
    "example-mcp-server-dev": {
      "command": "/Users/username/.nvm/versions/node/v22.16.0/bin/node",
      "args": [
        "/full/path/to/MCP_EXAMPLE_PROJECT/node_modules/.bin/tsx",
        "/full/path/to/MCP_EXAMPLE_PROJECT/src/index.ts",
        "--stdio"
      ],
      "cwd": "/full/path/to/MCP_EXAMPLE_PROJECT",
      "env": {
        "NODE_ENV": "development",
        "LOG_LEVEL": "debug"
      }
    },
    "example-mcp-server-simple": {
      "command": "node",
      "args": [
        "dist/index.js",
        "--stdio"
      ],
      "cwd": "/full/path/to/MCP_EXAMPLE_PROJECT",
      "env": {
        "NODE_ENV": "production"
      }
    }
  }
}
